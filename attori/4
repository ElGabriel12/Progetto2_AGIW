<script>
  history.replaceState('/person/4-carrie-fisher/remote/credits?credit_media_type=&credit_department=', document.title, document.location.href);

  function pushChangeUrl(url) {
    var historyUrl = url.replace('/remote/credits', '');
    history.pushState(url, null, historyUrl);
  }

  window.addEventListener('popstate', function(e) {
    if (e.state != null) {
      submitForm(e.state);
    }
  });

  function submitForm(url) {
    $.ajax({
      url: encodeURI(url),
      type: 'GET',
      dataType: 'html',
      beforeSend: function() {
       activatePageLoader();
      }
    }).fail(function() {
      showError('There was a problem.');
      deactivatePageLoader();
    }).done(function(response) {
      deactivatePageLoader();

      $('ul#filters li.reset.hide').removeClass('hide');
      $('div.credits_list').html(response);
      observer.observe();
    });
  }

  function checkAndSetBiographyHeight() {
    var biographyText = $("div.biography div.content div.text");
    var fullBiographyHeight = 0;
    biographyText.children().each(function() {
      fullBiographyHeight += $(this).outerHeight();
    });

    if ( biographyText.hasClass('truncate') ) {
      var displayedBiographyHeight = biographyText.outerHeight();

      if (fullBiographyHeight > displayedBiographyHeight) {
        biographyText.addClass('should_fade');
        $('div.read_more').removeClass('hide');
      } else {
        biographyText.removeClass('should_fade truncate');
        $('div.read_more').addClass('hide');
      }
    }
  }

  window.addEventListener("resize", checkAndSetBiographyHeight);

  $(document).ready(function() {
    initializeScroller(false, '#known_for_scroller');
    checkAndSetBiographyHeight();

    $('.account_adult_false.item_adult_true a').on('click', function(e) {
      e.preventDefault();
    });

    var w = window.innerWidth;
    var h = window.innerHeight;

    $('a.read_more').on('click', function(e) {
      $('div.read_more').addClass('hide');
      $("div.biography div.content div.text").removeClass('should_fade truncate');
    });

    $("#filters").kendoMenu({
      scrollable: false,
      animation: false,
      hoverDelay: 0,
      openOnClick: true
    });

    $('#filters').on('click', 'ul li.item_link a', function(e) {
      e.preventDefault();
      var url = $(this).attr('href');
      submitForm(url);
      pushChangeUrl(url);
      $("#filters").data("kendoMenu").close();
    });

    

    
      var media_tooltip_url_template = "{0}/static_cache/tooltip_popup?translate=false";

      $("table.credits").kendoTooltip({
        autoHide: false,
        filter: "span.glyphicons_v2:not(.account_adult_false.item_adult_true)",
        content: {
          url: "/movie/1/static_cache/tooltip_popup",
          cache: true
        },
        showOn: 'click',
        width: 534,
        height: 170,
        position: "top",
        requestStart: function(e) {
          e.options.url = kendo.format(media_tooltip_url_template, e.target.data("url"));
        },
        show: function(e) {
          $("div.k-tooltip-button").addClass('hide');
          this.popup.element.addClass("tmdb_theme flex");
        },
        contentLoad: function(e) {
          observer.observe();

          
            $("div.media_tooltip_popup div.buttons").addClass('hide');
          
        }
      });
    

  });
</script>
    </div>
  </section>
</main>


