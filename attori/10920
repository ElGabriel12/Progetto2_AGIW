        var window_image_dimensions = getImageWindowDimensions(h, 0.666);
      

      $('body').on('keyup', function(event) {
        switch(event.which) {
          case 27:
            $('a#close').click();
          break;

          case 37:
            $('#paging a.previous_id').click();
          break;

          case 39:
            $('#paging a.next_id').click();
          break;

          default: break;
        }
      });

      $('div.zoom a').on('click', function (e){
        e.preventDefault();
        
        
          var image_id = '5f8866633103250036f10abb';
        

        var lightbox_url_template = "/v4/image/{0}?translate=false&media_type=person&media_id=10920&image_type=poster&image_width={1}&image_height={2}";
