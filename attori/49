    <script id="slim_movie_result_template" type="text/x-kendo-tmpl">
      <div class="ac_item poster list_item">
        <div class="image_content poster">
          # if (data.poster_path) { #
            <img class="" src="//image.tmdb.org/t/p/w45_and_h67_bestv2${data.poster_path}" alt="${data.title}" width="45" height="67" srcset="//image.tmdb.org/t/p/w45_and_h67_bestv2${data.poster_path} 1x, //image.tmdb.org/t/p/w90_and_h134_bestv2${data.poster_path} 2x">
