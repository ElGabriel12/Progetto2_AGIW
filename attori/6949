      $('body').on('keyup', function(event) {
        switch(event.which) {
          case 27:
            $('a#close').click();
          break;

          case 37:
            $('#paging a.previous_id').click();
          break;

          case 39:
            $('#paging a.next_id').click();
          break;

          default: break;
        }
      });

      $('div.zoom a').on('click', function (e){
        e.preventDefault();
        
        
          var image_id = '5ccdcdb9c3a36836bede06b7';
        

