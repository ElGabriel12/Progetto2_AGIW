    <script id="person_template_with_known_for" type="text/x-kendo-tmpl">
      <div class="item profile list_item known_for">
        <div class="image_content profile">
          # if (data.profile_path) { #
            <img class="" src="//image.tmdb.org/t/p/w66_and_h66_face${data.profile_path}" alt="${data.name}" width="45" height="45" srcset="//image.tmdb.org/t/p/w66_and_h66_face${data.profile_path} 1x, //image.tmdb.org/t/p/w132_and_h132_face${data.profile_path} 2x">
