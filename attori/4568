            open: {
              effects: "fade:in"
            },
            close: {
              effects: "fade:out"
            }
          },
          activate: function() {
          },
          close: function() {
            $('body').removeClass('blur');
          },
          open: function(e) {
            e.sender.wrapper.addClass("borderless_window");
            $('body').addClass('blur');
          },
          refresh: function() {
            observer.observe();
          }
        }).data("kendoWindow").center().open();
      });
    
  });
